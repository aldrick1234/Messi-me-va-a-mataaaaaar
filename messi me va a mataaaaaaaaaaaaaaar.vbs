Option Explicit

Dim i

For i = 1 To 7
    Select Case i
        Case 1
            MsgBox "Messi me va a mataaaaar", 0 + 16, "XD"
        Case 2
            MsgBox "En un bosque de sal", 0 + 32, "XD"
        Case 3
            MsgBox "La grasa me salvo", 0 + 48, "XD"
        Case 4
            MsgBox "Solito hiba a nacer", 0 + 64, "XD"
        Case 5
            MsgBox "Solito me paso", 0 + 16, "XD"
        Case 6
            MsgBox "Solo con arena y mar", 0 + 32, "XD"
        Case 7
            MsgBox "Comi papel y sal", 0 + 48, "XD"
    End Select
Next
